package com.hdfc.test;

//Java Program to illustrate ConcurrentHashMap behaviour
import java.util.*;

public class HashMapDemo {
	public static void main(String[] args) {
		HashMap m = new HashMap();
		m.put(100, "Hello");
		m.put(null, "World");
		m.put(101, null);
		m.put(102, null);
		m.put(null, "Worlds");
		System.out.println(m);
	}
}
